<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%include ./common/css.ejs %>
  </head>
  <body>
    <%include ./common/header.ejs %>
    <div class="container">
        <div class="row">
            <table class="table table-hover table-bordered">
                <thead>
                    <tr>
                        <th>id</th>
                        <th>username</th>
                        <th>password</th>
                        <th>updatetime</th>
                    </tr>
                </thead>
                <tbody>
                <% users.forEach(function(item) { %>
                <tr class="item-id-<%=item._id%>">
                    <td><%=item._id%></td>
                    <td><%=item.name%></td>
                    <td><%=item.password%></td>
                    <td><%=moment(item.meta.updateAt).format('MM/DD/YYYY')%></td>
                </tr>
                <% }) %>
                </tbody>
            </table>
        </div>
    </div>

    <%include ./common/script.ejs %>
  </body>
</html>
